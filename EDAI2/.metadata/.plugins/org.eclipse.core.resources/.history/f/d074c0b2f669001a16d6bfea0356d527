package org.eda2;

import org.graalvm.compiler.graph.Graph.SourcePositionTracking;

public class Prubea {

	public static void main(String[] args) {

		Damero d = new Damero(4,4);

		System.out.println(d.toString());
		
		ParEdificios[] pE = d.lineaTroncal();
		
		for(int i = 0; i< 4; i++) {
			System.out.println("Fila " + i + " -> " +  pE[i]);
		}
		
	}
}
